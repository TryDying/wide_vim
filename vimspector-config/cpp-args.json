{
    "configurations": {
        "cpp:launch": {
            "adapter": "vscode-cpptools",
            "configuration": {
                "name": "cpp",
                "type":    "cppdbg",
                "request": "launch",
                "program": "${fileDirname}/${fileBasenameNoExtension}",
                "args": ["*${ProgramArgs}"],
                "cwd": "${workspaceRoot}",
                "environment": [],
                "externalConsole": true,
                "stopAtEntry": true,
                "MIMode": "gdb",
                "logging": {
                    "engineLogging": false
                }
            },
            "variables":{
                "User_vars":"value"
            }
        },
        "cpp:attach": {
            "adapter": "vscode-cpptools",
            "configuration": {
                "name": "cpp",
                "type": "cppdbg",
                "request": "attach",
                "program": "${fileDirname}/${fileBasenameNoExtension}",
                "MIMode": "gdb"
            }
        }
    }
}
